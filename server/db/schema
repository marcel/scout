CREATE TABLE "accounts" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "username" varchar(255), "password" text, "created_at" datetime, "updated_at" datetime);
CREATE TABLE "expert_ranks" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "yahoo_player_key" varchar(255), "position_type" varchar(255), "week" integer, "overall_rank" integer, "expert_1_rank" integer, "expert_2_rank" integer, "expert_3_rank" integer, "expert_4_rank" integer, "expert_5_rank" integer, "created_at" datetime, "updated_at" datetime);
CREATE TABLE "game_weeks" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "week" integer, "start_date" date, "end_date" date);
CREATE TABLE "games" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "week" integer, "season" integer, "away_team" varchar(255), "home_team" varchar(255), "kickoff_time" time, "created_at" datetime, "updated_at" datetime);
CREATE TABLE "injuries" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "fantasy_football_nerd_id" integer, "week" integer, "injury" varchar(255), "practice_status" varchar(255), "game_status" varchar(255), "last_update" date, "created_at" datetime, "updated_at" datetime);
CREATE TABLE "player_point_totals" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "yahoo_player_key" varchar(255), "season" integer, "total" float, "week" integer, "created_at" datetime, "updated_at" datetime);
CREATE TABLE "player_stat_values" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "yahoo_player_key" varchar(255), "season" integer, "week" integer, "yahoo_stat_id" integer, "value" float, "created_at" datetime, "updated_at" datetime);
CREATE TABLE "players" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "yahoo_key" varchar(255), "fantasy_football_nerd_id" integer, "full_name" varchar(255), "first_name" varchar(255), "last_name" varchar(255), "created_at" datetime, "updated_at" datetime, "playing_status" varchar(255), "team_full_name" varchar(255), "team_abbr" varchar(255), "bye_week" integer, "uniform_number" integer, "position" varchar(255), "position_type" varchar(255), "headshot" varchar(255), "owner_key" varchar(255), "waiver_date" date, "ownership_type" varchar(255));
CREATE TABLE "projections" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "fantasy_football_nerd_id" integer, "week" integer, "standard_high" float, "standard_low" float, "standard" float, "ppr_high" float, "ppr_low" float, "ppr" float, "rank" integer, "created_at" datetime, "updated_at" datetime);
CREATE TABLE "roster_adds" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "yahoo_team_key" varchar(255), "week" integer, "value" integer);
CREATE TABLE "roster_changes" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "yahoo_player_key" varchar(255), "type" varchar(255), "source_type" varchar(255), "source_team_key" varchar(255), "destination_type" varchar(255), "destination_team_key" varchar(255), "week" integer, "created_at" datetime, "updated_at" datetime);
CREATE TABLE "roster_spots" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "yahoo_team_key" varchar(255), "yahoo_player_key" varchar(255), "week" integer, "position" varchar(255), "playing_status" varchar(255), "created_at" datetime, "updated_at" datetime, "active" boolean DEFAULT 't');
CREATE TABLE "schema_migrations" ("version" varchar(255) NOT NULL);
CREATE TABLE "stats" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "name" varchar(255), "yahoo_stat_id" integer, "sort_order" integer, "position_type" varchar(255), "display_name" varchar(255));
CREATE TABLE "teams" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "yahoo_key" varchar(255), "name" varchar(255), "moves" integer, "trades" integer, "created_at" datetime, "updated_at" datetime, "logo" varchar(255), "division_id" integer, "waiver_priority" integer, "faab_balance" integer, "manager" varchar(255));
CREATE TABLE "transactions" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "yahoo_key" varchar(255), "type" varchar(255), "timestamp" integer, "add_roster_change_id" integer, "drop_roster_change_id" integer, "bid" integer, "created_at" datetime, "updated_at" datetime);
CREATE TABLE "watches" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "player_id" integer, "team_id" integer, "created_at" datetime, "updated_at" datetime, "votes" integer DEFAULT 1);
CREATE INDEX "index_game_weeks_on_week_and_start_date" ON "game_weeks" ("week", "start_date");
CREATE INDEX "index_injuries_on_fantasy_football_nerd_id" ON "injuries" ("fantasy_football_nerd_id");
CREATE INDEX "index_injuries_on_week" ON "injuries" ("week");
CREATE INDEX "index_injuries_on_week_and_game_status" ON "injuries" ("week", "game_status");
CREATE INDEX "index_injuries_on_week_and_injury" ON "injuries" ("week", "injury");
CREATE INDEX "index_injuries_on_week_and_practice_status" ON "injuries" ("week", "practice_status");
CREATE INDEX "index_player_point_totals_on_season" ON "player_point_totals" ("season");
CREATE INDEX "index_player_point_totals_on_season_and_week" ON "player_point_totals" ("season", "week");
CREATE INDEX "index_player_point_totals_on_total" ON "player_point_totals" ("total");
CREATE INDEX "index_player_point_totals_on_week" ON "player_point_totals" ("week");
CREATE INDEX "index_player_point_totals_on_yahoo_player_key" ON "player_point_totals" ("yahoo_player_key");
CREATE INDEX "index_player_stat_values_on_week" ON "player_stat_values" ("week");
CREATE INDEX "index_player_stat_values_on_yahoo_player_key" ON "player_stat_values" ("yahoo_player_key");
CREATE INDEX "index_player_stat_values_on_yahoo_stat_id" ON "player_stat_values" ("yahoo_stat_id");
CREATE INDEX "index_players_on_fantasy_football_nerd_id" ON "players" ("fantasy_football_nerd_id");
CREATE INDEX "index_players_on_owner_key" ON "players" ("owner_key");
CREATE INDEX "index_players_on_ownership_type" ON "players" ("ownership_type");
CREATE INDEX "index_players_on_waiver_date" ON "players" ("waiver_date");
CREATE INDEX "index_players_on_yahoo_key" ON "players" ("yahoo_key");
CREATE INDEX "index_projections_on_fantasy_football_nerd_id" ON "projections" ("fantasy_football_nerd_id");
CREATE INDEX "index_projections_on_week" ON "projections" ("week");
CREATE INDEX "index_projections_on_week_and_standard" ON "projections" ("week", "standard");
CREATE INDEX "index_roster_changes_on_destination_team_key" ON "roster_changes" ("destination_team_key");
CREATE INDEX "index_roster_changes_on_source_team_key" ON "roster_changes" ("source_team_key");
CREATE INDEX "index_roster_changes_on_yahoo_player_key" ON "roster_changes" ("yahoo_player_key");
CREATE INDEX "index_roster_spots_on_week" ON "roster_spots" ("week");
CREATE INDEX "index_roster_spots_on_yahoo_player_key" ON "roster_spots" ("yahoo_player_key");
CREATE INDEX "index_roster_spots_on_yahoo_team_key" ON "roster_spots" ("yahoo_team_key");
CREATE INDEX "index_roster_spots_on_yahoo_team_key_and_week" ON "roster_spots" ("yahoo_team_key", "week");
CREATE INDEX "index_stats_on_yahoo_stat_id" ON "stats" ("yahoo_stat_id");
CREATE INDEX "index_teams_on_yahoo_key" ON "teams" ("yahoo_key");
CREATE INDEX "index_transactions_on_type" ON "transactions" ("type");
CREATE INDEX "index_transactions_on_yahoo_key" ON "transactions" ("yahoo_key");
CREATE INDEX "index_watches_on_team_id_and_player_id" ON "watches" ("team_id", "player_id");
CREATE INDEX "key_week_position_type" ON "expert_ranks" ("yahoo_player_key", "week", "position_type");
CREATE UNIQUE INDEX "unique_schema_migrations" ON "schema_migrations" ("version");
