<div class="page-header">
  <h1><%=t '.title', :default => 'Players' %></h1>
</div>
<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th class="span1">Rank</th>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
    <% @projections.each do |projection| %>
      <tr>
        <td class="span1"><%= projection.rank %></td>
        <td>
          <%= player_profile(projection.player, player_path(projection.player)) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
